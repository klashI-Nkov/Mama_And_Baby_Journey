<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:showIn="@layout/layouts_doctor_section_doctor_activity_app_bar_layout"

    android:theme="@style/screen_Background"
    style="@style/screen_Background"

    android:layoutDirection="rtl">

    <fragment
        android:id="@+id/doctor_activity_nav_host"
        android:name="androidx.navigation.fragment.NavHostFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:defaultNavHost="true"
        app:navGraph="@navigation/doctor_section_doctor_activity_mobile_navigation"
        tools:ignore="FragmentTagUsage"/>

</androidx.constraintlayout.widget.ConstraintLayout>